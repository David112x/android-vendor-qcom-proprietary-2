#ENABLE DISCOVERY ONLY ON qcs605
ifeq ($(TARGET_BOARD_PLATFORM),qcs605)

LOCAL_PATH:= $(call my-dir)
include $(CLEAR_VARS)

LOCAL_SRC_FILES := discoveryd.c

LOCAL_MODULE:= discoveryd
LOCAL_MODULE_TAGS := optional
LOCAL_SHARED_LIBRARIES := libutils
LOCAL_SHARED_LIBRARIES += libcutils
LOCAL_SHARED_LIBRARIES += liblog
LOCAL_CFLAGS += -DFEATURE_DATA_LINUX_ANDROID
LOCAL_PROPRIETARY_MODULE := true

LOCAL_C_INCLUDES := $(TARGET_OUT_HEADERS)/common/inc/
LOCAL_C_INCLUDES += $(TARGET_OUT_HEADERS)/data/inc/

LOCAL_MODULE := discoveryd
include $(BUILD_EXECUTABLE)

############### default config file ################

include $(CLEAR_VARS)
LOCAL_MODULE := discoveryd.config
LOCAL_MODULE_CLASS := ETC
LOCAL_MODULE_PATH := $(TARGET_OUT_VENDOR_ETC)
LOCAL_MODULE_OWNER := qti
LOCAL_SRC_FILES := $(LOCAL_MODULE)
include $(BUILD_PREBUILT)

endif #ENABLE DISCOVERY ONLY ON qcs605
